{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true,
		"noImplicitAny": false,
		// "types": ["next", "next-auth", "estree"],
		"types": ["estree"],
		"target": "es6",
		"noUnusedLocals": false,
		"lib": ["dom", "dom.iterable", "esnext", "webworker"],
		"typeRoots": ["./node_modules/@types"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		// "noUnusedParameters": false,
		// "noFallthroughCasesInSwitch": false,
		// "strictNullChecks": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./src/*"],
			"@/lib/*": ["./src/lib/*"]
		},
		"baseUrl": "."
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"src/types/**/*.ts",
		"src/types/change/fabric-types.ts",
		"src/app/api/generate-video-with-audio",
		"src/components/modify/GlassModalWrappertsx",
		"src/app/api/modify/auth/route_0.ts",
		"out/types/**/*.ts",
		"src/components/spinner"
	],
	"exclude": [
		"node_modules",
		"surveyjs-core/*",
		"**/*.test.ts",
		"dawn-tree-2ef5/node_modules",
		"frosty-brook-3aae/node_modules"
	]
}
